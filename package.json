{
  "name": "npm-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "setup": "yarn install",
    "build": "tsc",
    "dist": "tsc --declaration --module commonjs --target es6 --esModuleInterop",
