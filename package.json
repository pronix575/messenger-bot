    "prepublish": "yarn build"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "config": "^3.3.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.6"
  }
}
